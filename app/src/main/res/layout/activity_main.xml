<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.josiah.budgetapp1.MainActivity">

    <ListView
        android:id="@+id/bill_list_view"
        android:layout_width="368dp"
        android:layout_height="495dp"
        app:layout_constraintLeft_toRightOf="@id/activity_main"
        android:layout_marginLeft="0dp"
        app:layout_constraintRight_toLeftOf="@id/activity_main"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@id/activity_main"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@id/activity_main"></ListView>

</android.support.constraint.ConstraintLayout>
